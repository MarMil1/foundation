{
	"name": "stackoverflow-ide",
	"displayName": "StackOverflow IDE",
	"description": "Search questions through StackOverFlow IDE",
	"version": "0.0.1",
	"private": true,
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:stackoverflow-ide.run"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "stackoverflow-ide.run",
				"title": "StackOverflow IDE"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "npm-run-all compile:*",
		"compile:vue": "vue-cli-service build --mode=development --inline-vue",
		"compile:extension": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "mocha ./src/test/ --recursive"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.44.0",
		"@typescript-eslint/eslint-plugin": "^2.26.0",
		"@typescript-eslint/parser": "^2.26.0",
		"@vue/cli-plugin-typescript": "^4.3.1",
		"@vue/cli-service": "^4.3.1",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.1",
		"npm-run-all": "^4.1.5",
		"typescript": "^3.8.3",
		"vscode": "^1.1.37",
		"vscode-test": "^1.3.0",
		"vue-template-compiler": "^2.6.11"
	},
	"dependencies": {
		"axios": "^0.19.2",
		"common.js": "^1.1.1",
		"vue": "^2.6.11",
		"vue-class-component": "^7.2.3",
		"vue-property-decorator": "^8.4.2",
		"vuetify": "^2.2.28"
	}
}
